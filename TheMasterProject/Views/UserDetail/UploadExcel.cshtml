
@{
    ViewBag.Title = "UploadExcel";
    Layout = "~/Views/Shared/_SBAdminMaster.cshtml";
}

@using (Html.BeginForm("UploadExcel", "UserDetail", FormMethod.Post, new { enctype = "multipart/form-data", onsubmit = "return myFunction()" }))
{
    <div class="row">
        <div class="control-label col-md-2">Excel:</div>
        <div class="col-md-10">
            <input type="file" id="FileUpload" name="FileUpload" class="" />
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Upload" id="btnSubmit" class="btn btn-primary" />
            </div>
        </div>
    </div>

}
<div id="diverrorMessage" class="alert alert-warning" role="alert">
    @ViewBag.Message
</div>

@section Scripts{
    <script>
        $("#diverrorMessage").fadeTo(5000, 500).slideUp(500, function () {
            $("#diverrorMessage").slideUp(500);
        });
    </script>    
}